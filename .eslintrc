{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": false
		}
	},
	"rules": {
		"semi": "warn",
		"quotes": "off",
		"array-bracket-spacing": "warn",
		"prefer-rest-params": "warn",
		"arrow-body-style": "off"
	},
	"env": {
		"es6": true
	},
	"parser": "babel-eslint"
}
